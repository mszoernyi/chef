{
  "name": "base",
  "default_attributes": {
    "chef_client": {
      "version": "12.18.31",
      "log_file": null
    },
    "chefdk": {
      "version": "1.3.10"
    },
    "ohai": {
      "plugin_path": "/etc/chef/ohai/plugins",
      "disabled_plugins": [
        ":Hostnamectl"
      ]
    },
    "packages": {
      "debian": [
        "acct",
        "acpitool",
        "atool",
        "blktool",
        "bmon",
        "btrfs-tools",
        "bwm-ng",
        "colordiff",
        "conntrack",
        "diffstat",
        "dnsutils",
        "dos2unix",
        "dstat",
        "e2fsprogs",
        "edac-utils",
        "ethtool",
        "fuse",
        "hddtemp",
        "hdparm",
        "iotop",
        "ipcalc",
        "ipmitool",
        "ipv6calc",
        "jq",
        "latencytop",
        "lft",
        "linux-tools-generic",
        "lshw",
        "lsof",
        "ltrace",
        "lvm2",
        "mc",
        "mdadm",
        "mercurial",
        "mtr-tiny",
        "ncdu",
        "nethogs",
        "netstat-nat",
        "ngrep",
        "nmap",
        "numactl",
        "parallel",
        "parted",
        "patchutils",
        "pciutils",
        "procps",
        "psmisc",
        "pv",
        "pwgen",
        "pydf",
        "schedtool",
        "silversearcher-ag",
        "smartmontools",
        "socat",
        "strace",
        "sysstat",
        "tcpdump",
        "tcptraceroute",
        "time",
        "tiptop",
        "traceroute",
        "tree",
        "unzip",
        "whois"
      ]
    }
  },
  "override_attributes": {
    "authorization": {
      "sudo": {
        "groups": [
          "sudo"
        ],
        "passwordless": true
      }
    }
  },
  "run_list": [
    "recipe[base]",
    "recipe[bash]",
    "recipe[cron]",
    "recipe[git]",
    "recipe[gnupg]",
    "recipe[htop]",
    "recipe[locale]",
    "recipe[logrotate]",
    "recipe[ssh]",
    "recipe[sshd]",
    "recipe[tmux]",
    "recipe[vim]"
  ]
}
